package br.com.ecommerce.modelo;

public class Genero {

	private int generoId;
	private String nome;
	public int getGeneroId() {
		return generoId;
	}
	public void setGeneroId(int generoId) {
		this.generoId = generoId;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	
	
}
